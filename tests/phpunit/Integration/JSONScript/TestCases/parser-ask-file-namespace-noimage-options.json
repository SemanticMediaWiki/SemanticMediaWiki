{
	"description": "Test `#ask`/ NS_FILE option, `noimage` (`wgEnableUploads`, `wgFileExtensions`, `wgDefaultUserOptions`)",
	"setup": [
		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "Has text",
			"contents": "[[Has type::Text]]"
		},
		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "Has file",
			"contents": "[[Has type::Page]]"
		},
		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "Has text",
			"contents": "[[Has page::Page]]"
		},
		{
			"namespace": "NS_FILE",
			"page": "PA0004.png",
			"contents": {
				"upload": {
					"file" : "/../Fixtures/image-upload-480.png",
					"text" : "[[Has file::{{FULLPAGENAME}}]] [[Has caption::123]] {{#subobject:Test|Has text=Foo}}"
				}
			}
		},
		{
			"page": "Example/PA0004/Q.1",
			"contents": "{{#ask: [[Has file::+]] |?Has file |format=table |limit=1 }}"
		},
		{
			"page": "Example/PA0004/Q.2",
			"contents": "{{#ask: [[Has file::+]] |?Has file#120px;thumb;<b>{{#show: File:PA0004.png |?Has caption |link=none}}</b>[[Extra]] |format=table |limit=1 }}"
		},
		{
			"page": "Example/PA0004/Q.3",
			"contents": "{{#ask: [[File:+]] }}"
		},
		{
			"page": "Example/PA0004/4",
			"contents": "[[Has page::File:PA0004.png]]"
		},
		{
			"page": "Example/PA0004/Q.4",
			"contents": "{{#ask: [[Has page::<q>[[File:+]]</q>]] |?Has page#noimage |format=table |limit=1 }}"
		}
	],
	"tests": [
		{
			"type": "parser",
			"about": "#0",
			"subject": "Example/PA0004/Q.1",
			"assert-output": {
				"to-contain": [
					"href=\"/index.php/File:PA0004.png\"",
					"300px-PA0004.png",
					"width=\"300\" height=\"300\""
				],
				"not-contain": [
					"PA0004.png\" width=\"480\" height=\"480\""
				]
			}
		},
		{
			"type": "parser",
			"about": "#1 with options",
			"subject": "Example/PA0004/Q.2",
			"assert-output": {
				"to-contain": [
					"300px-PA0004.png",
					"width=\"300\" height=\"300\"",
					"120px-PA0004.png\"",
					"width=\"120\" height=\"120\"",
					"123",
					"Extra"
				]
			}
		},
		{
			"type": "parser",
			"about": "#2 (subobject is displayed as normal link and not with an image reference)",
			"subject": "Example/PA0004/Q.3",
			"assert-output": {
				"to-contain": [
					"<span class=\"smw-subobject-entity\"><a href=.*PA0004.png#Test\" title=.*PA0004.png\">PA0004.png#Test</a></span>",
					"<a href=.*PA0004.png\""
				]
			}
		},
		{
			"type": "parser",
			"about": "#3 (#noimage)",
			"subject": "Example/PA0004/Q.4",
			"assert-output": {
				"to-contain": [
					"<td class=\"smwtype_wpg\"><a href=\".*Example/PA0004/4\" title=\"Example/PA0004/4\">Example/PA0004/4</a></td><td class=\"Has-page smwtype_wpg\"><a href=\".*PA0004.png\" title=\".*PA0004.png\">PA0004.png</a></td>"
				],
				"not-contain": [
					"PA0004.png\" width=\"480\" height=\"480\""
				]
			}
		}
	],
	"settings": {
		"wgContLang": "en",
		"wgLang": "en",
		"wgEnableUploads": true,
		"wgFileExtensions": [
			"png"
		],
		"wgDefaultUserOptions": {
			"thumbsize": 5
		},
		"smwgPageSpecialProperties": [
			"_MDAT"
		],
		"smwgNamespacesWithSemanticLinks": {
			"NS_MAIN": true,
			"NS_FILE": true
		}
	},
	"meta": {
		"skip-on": {
			"sitelanguage": [ "ja", "Some issues with the thumb output when 'ja' is used as sitelanguage." ]
		},
		"version": "2",
		"is-incomplete": false,
		"debug": false
	}
}