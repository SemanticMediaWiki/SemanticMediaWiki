			"api.browse.pvalue": false,
			"api.browse.psubject": false
		},
		"smwgNamespacesWithSemanticLinks": {
			"NS_MAIN": true,
			"SMW_NS_PROPERTY": true,
			"SMW_NS_CONCEPT": true,
			"NS_CATEGORY": true
		}
