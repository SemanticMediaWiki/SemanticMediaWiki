		},
		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "API date property",
			"contents": "[[Has type::Date]]"
		},
		{
			"namespace": "SMW_NS_CONCEPT",
			"page": "API test concept",
			"contents": "..."
