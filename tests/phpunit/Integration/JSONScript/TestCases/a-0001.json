			"api": {
				"parameters": {
					"action": "smwbrowse",
					"format": "json",
					"browse": "psubject",
					"params": "{ \"limit\": 10, \"offset\": 0, \"property\": \"API page property\", \"value\": \"\", \"sort\": \"asc\" }"
				}
			},
			"assert-output": {
