{
	"description": "Test sorting on Pages will not exclude non-existent pages from result (#540)",
	"setup": [
		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "knows page",
			"contents": "[[Has type::Page]]"
		},
		{
			"page": "Example/PS0001/1",
			"contents": "[[knows page::Example/PS0001/Ref 3]]"
		},
		{
			"page": "Example/PS0001/2",
			"contents": "[[knows page::Example/PS0001/Ref 2]]"
		},
		{
			"page": "Example/PS0001/3",
			"contents": "[[knows page::Example/PS0001/Ref 1]]"
		},
		{
			"page": "Example/PS0001/Ref 1",
			"contents": "SomeContent"
		},
		{
			"page": "Example/PS0001/Ref 3",
			"contents": "SomeContent"
		},
		{
			"page": "Example/PS0001/Test",
			"contents": "{{#ask:[[knows page::+]]|?knows page |sort=knows page|format=plainlist}}"
		}
