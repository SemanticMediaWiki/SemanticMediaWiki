		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "Has text",
			"contents": "[[Has type::Text]]"
		},
		{
			"page": "Example/PA0042/1",
			"contents": "[[Has text:=Lorem ipsum]]"
